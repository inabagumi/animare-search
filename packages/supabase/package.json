{
  "author": "Haneru Developers",
  "dependencies": {
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.39.8"
  },
  "description": "",
  "devDependencies": {
    "@shinju-date/eslint-config": "workspace:*",
    "@tsconfig/node20": "^20.1.3",
    "@tsconfig/strictest": "^2.0.3",
    "@types/cookie": "^0.6.0",
    "eslint": "^8.57.0",
    "next": "^14.1.4",
    "supabase": "^1.150.0",
    "tsup": "^8.0.2",
    "typescript": "~5.4.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "react-server": "./dist/server.js",
      "default": "./dist/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "default": "./dist/middleware.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    }
  },
  "homepage": "https://shinju.date/",
  "license": "MIT",
  "main": "./dist/index.js",
  "name": "@shinju-date/supabase",
  "peerDependencies": {
    "next": "^14.1.4"
  },
  "private": true,
  "repository": {
    "directory": "packages/supabase",
    "type": "git",
    "url": "https://github.com/inabagumi/shinju-date.git"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > types/supabase.ts",
    "lint": "eslint ."
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "./middleware": [
        "./dist/middleware.d.ts"
      ],
      "./server": [
        "./dist/server.d.ts"
      ]
    }
  },
  "version": "1.0.0"
}
